{
  "name": "@rss-proxy/proxy",
  "version": "1.0.3",
  "private": true,
  "dependencies": {
    "@rss-proxy/core": "file:../core",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dompurify": "^2.3.0",
    "express": "^4.17.1",
    "feed": "^4.2.2",
